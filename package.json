{
  "name": "chess-com-insights",
  "private": true,
  "version": "2.4.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/thieleju/chess-com-insights"
  },
  "scripts": {
    "prebuild": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "tsc && ts-mocha",
    "test:junit": "tsc && ts-mocha --reporter mocha-junit-reporter --reporter-options mochaFile=tests/results.xml",
    "cover": "npx c8 npm run test && npx c8 report --reporter html"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "luxon": "^3.4.4",
    "roboto-fontface": "*",
    "vue": "^3.4.14",
    "vuetify": "^3.4.10",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.22",
    "@types/chai": "^4.3.11",
    "@types/chrome": "^0.0.256",
    "@types/jsdom": "^21.1.6",
    "@types/jsdom-global": "^3.0.7",
    "@types/luxon": "^3.4.1",
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.11.4",
    "@types/sinon": "^17.0.3",
    "@types/webfontloader": "^1.6.37",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "c8": "^9.1.0",
    "chai": "^5.0.0",
    "jsdom": "^23.2.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.2.1",
    "sinon": "^17.0.1",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.1",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.8.27"
  }
}
