{
  "name": "chess-com-insights",
  "private": true,
  "version": "2.6.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/thieleju/chess-com-insights"
  },
  "scripts": {
    "prebuild": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "tsc && ts-mocha",
    "test:junit": "tsc && ts-mocha --reporter mocha-junit-reporter --reporter-options mochaFile=tests/results.xml",
    "cover": "npx c8 npm run test && npx c8 report --reporter html"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "luxon": "^3.4.4",
    "roboto-fontface": "*",
    "vue": "^3.4.31",
    "vuetify": "^3.6.12",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.25",
    "@types/chai": "^4.3.16",
    "@types/chrome": "^0.0.268",
    "@types/jsdom": "^21.1.7",
    "@types/jsdom-global": "^3.0.7",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@types/sinon": "^17.0.3",
    "@types/webfontloader": "^1.6.37",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "jsdom": "^24.1.0",
    "mocha": "^10.6.0",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.3.3",
    "sinon": "^18.0.0",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^4.5.2",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^2.0.26"
  }
}
