{
  "name": "chess-com-insights",
  "private": true,
  "version": "2.3.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/thieleju/chess-com-insights"
  },
  "scripts": {
    "prebuild": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "tsc && ts-mocha",
    "test:junit": "tsc && ts-mocha --reporter mocha-junit-reporter --reporter-options mochaFile=tests/results.xml",
    "cover": "npx c8 npm run test && npx c8 report --reporter html"
  },
  "dependencies": {
    "@mdi/font": "7.3.67",
    "luxon": "^3.4.4",
    "roboto-fontface": "*",
    "vue": "^3.3.8",
    "vuetify": "^3.4.2",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.21",
    "@types/chai": "^4.3.10",
    "@types/chrome": "^0.0.251",
    "@types/jsdom": "^21.1.5",
    "@types/jsdom-global": "^3.0.7",
    "@types/luxon": "^3.3.4",
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.9.3",
    "@types/sinon": "^17.0.1",
    "@types/webfontloader": "^1.6.37",
    "@vitejs/plugin-vue": "^4.5.0",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.1.0",
    "sinon": "^17.0.1",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.8.22"
  }
}
