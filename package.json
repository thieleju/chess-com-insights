{
  "name": "chess-com-insights",
  "private": true,
  "version": "2.7.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/thieleju/chess-com-insights"
  },
  "scripts": {
    "prebuild": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "tsc && ts-mocha",
    "test:junit": "tsc && ts-mocha --reporter mocha-junit-reporter --reporter-options mochaFile=tests/results.xml",
    "cover": "npx c8 npm run test && npx c8 report --reporter html"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "luxon": "^3.7.2",
    "vue": "^3.5.24",
    "vuetify": "^3.10.11"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.2.1",
    "@eslint/js": "^9.39.1",
    "@types/chai": "^5.0.1",
    "@types/chrome": "^0.1.30",
    "@types/eslint__js": "^9.14.0",
    "@types/jsdom": "^27.0.0",
    "@types/jsdom-global": "^3.0.7",
    "@types/luxon": "^3.7.1",
    "@types/mocha": "^10.0.9",
    "@types/node": "^24.10.1",
    "@types/sinon": "^21.0.0",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "c8": "^10.1.2",
    "chai": "^6.2.1",
    "eslint": "^9.39.1",
    "jsdom": "^27.2.0",
    "mocha": "^11.7.5",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^3.6.2",
    "sinon": "^21.0.0",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-tsc": "^3.1.4"
  }
}